{
  "name": "match-like-gulp",
  "version": "0.0.1",
  "description": "Check a string matches glob or not like Gulp",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require should --reporter spec",
    "test-cov": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should",
    "test-travis": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --require should"
  },
  "keywords": [
    "match",
    "gulp",
    "glob"
  ],
  "author": "lingyucoder",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "~0.3.21",
    "mocha": "~2.3.3",
    "should": "~7.1.0"
  },
  "dependencies": {
    "minimatch": "~3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LingyuCoder/match-like-gulp.git"
  },
  "bugs": {
    "url": "https://github.com/LingyuCoder/match-like-gulp/issues"
